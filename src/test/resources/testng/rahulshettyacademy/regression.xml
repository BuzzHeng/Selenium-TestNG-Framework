<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Regression Test Suite" verbose="1" parallel="methods" thread-count="3">

    <listeners>
        <listener class-name="com.buzzheng.TestComponents.TestListener"/>
    </listeners>

    <test name="Regression Tests">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>

        <packages>
            <package name="com.buzzheng.rahulshettyacademy.*"/>
        </packages>
    </test>

</suite>