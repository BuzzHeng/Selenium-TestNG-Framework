<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="E2E Test Suite" verbose="1">

    <listeners>
        <listener class-name="com.buzzheng.TestComponents.TestListener"/>
    </listeners>

    <test name="E2E Tests">
        <groups>
            <run>
                <include name="e2e"/>
            </run>
        </groups>

        <packages>
            <package name="com.buzzheng.rahulshettyacademy.*"/>
        </packages>
    </test>

</suite>